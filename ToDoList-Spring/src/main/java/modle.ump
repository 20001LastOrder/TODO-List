namespace todo.model;



class TaskList{
  int tasksComplete = 0;
  int tasksPassedDueDate=0;
  1 -> * Category;
}

class Task{
  name;
  Date endDate;
  int priority;
  boolean complite = false;
  boolean passedDueDate = false;
  descriprion;
  lazy location;
  lazy note;
  0..1 patrentTask -> * Task subtasks;
}

class Category{
  name;
  1 -> * Task;
    0..1 parentCatagory -> * Category subcategory;
}